{
  "name": "kreyu/nbp-web-api",
  "type": "library",
  "description": "NBP Web API client for PHP.",
  "keywords": ["nbp", "api", "client"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Wr√≥blewski",
      "email": "kontakt@swroblewski.pl"
    }
  ],
  "require": {
    "php": "^7.1",
    "php-http/cache-plugin": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/client-common": "^1.6 || ^2.0",
    "php-http/discovery": "^1.12",
    "php-http/httplug": "^2.1",
    "psr/cache": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0",
    "php-http/guzzle6-adapter": "^2.0.1",
    "guzzlehttp/psr7": "^1.5.2"
  },
  "autoload": {
    "psr-4": {
      "Kreyu\\NBPWebApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kreyu\\NBPWebApi\\Tests\\": "tests"
    }
